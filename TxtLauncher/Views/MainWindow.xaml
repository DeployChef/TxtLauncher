<Window x:Class="TxtLauncher.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:TxtLauncher.Views"
        mc:Ignorable="d"
        Title="TxtLauncher" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="59*"/>
            <ColumnDefinition Width="337*"/>
        </Grid.ColumnDefinitions>
        <views:LauncherPanel Grid.ColumnSpan="2"></views:LauncherPanel>
    </Grid>
</Window>
